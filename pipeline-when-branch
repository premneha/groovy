pipeline {
    agent {
        node {
          label "slave1"
          customWorkspace "/root/Jenkins/testingbranch"
            }
            }
    options {
                 timestamps()
            }
    
    stages {
        stage("Build Master") {
            when {
                branch 'master'
            }
            steps {
                echo "Building master"
            }
        }
        stage("Build Dev") {
            when {
                branch 'dev'
            }
            steps {
                echo "Building dev"
            }
        }
    }

